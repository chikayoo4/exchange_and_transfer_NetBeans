	<parameter name="invoice_id" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     exc_invoice_tb."id_invoice" AS exc_invoice_tb_id_invoice,
     exc_invoice_tb."invoice_date" AS exc_invoice_tb_invoice_date,
     exc_invoice_tb."exchanging_money" AS exc_invoice_tb_exchanging_money,
     exc_invoice_tb."result_exchanging_money" AS exc_invoice_tb_result_exchanging_money,
     exc_invoice_tb."exchange_rate" AS exc_invoice_tb_exchange_rate,
     exc_invoice_tb."id_type" AS exc_invoice_tb_id_type,
     exc_invoice_tb."id_acc" AS exc_invoice_tb_id_acc,
     exc_invoice_tb."id_pur" AS exc_invoice_tb_id_pur,
     account_tb."id_acc" AS account_tb_id_acc,
     account_tb."user_name" AS account_tb_user_name,
     account_tb."password" AS account_tb_password,
     project_path."id_proj" AS project_path_id_proj,
     project_path."path_name" AS project_path_path_name,
     invoice_management_tb."id_invoice_man" AS invoice_management_tb_id_invoice_man,
     invoice_management_tb."id_invoice" AS invoice_management_tb_id_invoice,
     invoice_management_tb."invoice_man_date" AS invoice_management_tb_invoice_man_date,
     invoice_management_tb."rial" AS invoice_management_tb_rial,
     invoice_management_tb."dollar" AS invoice_management_tb_dollar,
     invoice_management_tb."bart" AS invoice_management_tb_bart,
     invoice_management_tb."bank_bart" AS invoice_management_tb_bank_bart,
     invoice_management_tb."id_acc" AS invoice_management_tb_id_acc,
     invoice_management_tb."id_pur" AS invoice_management_tb_id_pur,
         (SELECT invoice_management_tb."id_invoice_man" 
	FROM "dbo"."invoice_management_tb" 
	WHERE invoice_management_tb."id_acc" = exc_invoice_tb."id_acc"
	AND invoice_management_tb."id_pur" = exc_invoice_tb."id_pur"
	AND invoice_management_tb."id_invoice" = exc_invoice_tb."id_invoice") AS id_inv_man
FROM
     "dbo"."account_tb" account_tb FULL OUTER JOIN "dbo"."exc_invoice_tb" exc_invoice_tb ON account_tb."id_acc" = exc_invoice_tb."id_acc"
     FULL OUTER JOIN "dbo"."invoice_management_tb" invoice_management_tb ON account_tb."id_acc" = invoice_management_tb."id_acc",
     "dbo"."project_path" project_path
WHERE
     exc_invoice_tb."id_invoice" = $P{invoice_id}]]>
	</queryString>
